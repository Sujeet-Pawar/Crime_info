{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "API_URL": "https://api.example.com",
        "ANOTHER_ENV_VAR": "value"
    }
    "build": {
        "env": {
            "API_URL": "https://api.example.com",
            "ANOTHER_ENV_VAR": "value"
        }
    },
    "functions": {
        "api/**/*.js": {
            "maxDuration": 10,
            "memory": 1024
        }
    },
    "regions": ["sfo1", "cdg1"],
    "alias": "my-app-alias"
}